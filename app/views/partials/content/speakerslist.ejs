<article class="speakerslist">
    <div class="container">
        <div class="row">
            <% speakers.forEach(function(speaker) { %>
            <div class="col-sm-8">
                <h3 class="speakerslist-title"> <%= speaker.title %> </h3>
                <h5 class="speakerslist-name"> with <a href="/speakers/<%= speaker.shortname %>"> <%= speaker.name %></a></h5>
                <p class="speakerslist-info"><a href="/speakers/<%= speaker.shortname %>">
                        <img class="speakerslist-img img-circle pull-left" src="/images/speakers/<%= speaker.shortname %>_tn.jpg" alt="Photo of name"></a>
                    <%= speaker.summary %>
                    </p>
            </div>
            <% }) %>
        </div><!-- row -->
    </div><!-- container -->
</article>
